{
    "name": "express",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./server/bin/www& && node client/server.js --env=dev",
        "clean": "rimraf dist/*",
        "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
        "dist": "npm run copy & webpack --env=dist",
        "lint": "eslint ./src",
        "serve": "node server.js --env=dev",
        "serve:dist": "node server.js --env=dist"

    },
    "dependencies": {
        "body-parser": "~1.15.2",
        "cookie-parser": "~1.4.3",
        "debug": "~2.2.0",
        "express": "~4.14.0",
        "jade": "~1.11.0",
        "morgan": "~1.7.0",
        "serve-favicon": "~2.3.0",
        "core-js": "^2.0.0",
        "normalize.css": "^4.0.0",
        "react": "^15.0.0",
        "react-dom": "^15.0.0"
    },

  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "bower-webpack-plugin": "^0.1.9",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.0",
